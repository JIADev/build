<?xml version="1.0"?>
<cb:config-template xmlns:cb="urn:ccnet.config.builder">
  
  <!-- default queue-priority = 2000.  Subtract 1000 if it is part of a track build.  Subtract 50 for each dependency it has -->
  
  <cb:define name="queue1b">
    <cb:define queueName="Queue1" />
  </cb:define>
  
  <cb:define name="queue1d">
    <cb:define queueName="Queue1" />
    <cb:reqBuild customer="2096" req-number="REQ001" />
  </cb:define>
  
  <cb:define name="queue2b">
    <cb:define queueName="Queue2" />
    <cb:reqBuild customer="2096" req-number="RM53069" />
  </cb:define>
  
  <cb:define name="queue2d">
    <cb:define queueName="Queue2" />
    <cb:reqBuild customer="2096" req-number="REQ006" />
  </cb:define>
  
  <cb:define name="queue3b">
    <cb:define queueName="Queue3" />
  </cb:define>
  
  <cb:define name="queue3d">
    <cb:define queueName="Queue3" />
    <cb:reqBuild customer="2096" req-number="REQ004" />
  </cb:define>
  
  <cb:define name="build2queues">
    <cb:queue1b />
    <cb:queue2b />
    <cb:queue3b />
  </cb:define>
  
  <cb:define name="build4queues">
    <cb:queue1d />
    <cb:queue2d />
    <cb:queue3d />
  </cb:define>
  
</cb:config-template>
