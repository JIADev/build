<?xml version="1.0"?>
<cb:config-template xmlns:cb="urn:ccnet.config.builder">

  <!-- default queue-priority = 2000.  Subtract 1000 if it is part of a track build.  Subtract 50 for each dependency it has -->
  
  <cb:define name="track1">
    <cb:define track="1" />
    <cb:define even-odd="Odd" />
    <cb:reqBuild req-number="REQ089" />
    <cb:reqBuild req-number="REQ091" />
    <cb:reqBuild req-number="REQ086" />
    <cb:reqBuild req-number="REQ095" />
  </cb:define>
  
  <cb:define name="track2">
    <cb:define track="2" />
    <cb:define even-odd="Even" />
    <cb:reqBuild req-number="REQ128" />
    <cb:reqBuild req-number="REQ142" />
    <cb:reqBuild req-number="REQ143" />
  </cb:define>

  <cb:define name="track3">
    <cb:define track="3" />
    <cb:define even-odd="Odd" />
    <cb:reqBuild req-number="REQ024" />
  </cb:define>
  
  <cb:define name="track4">
    <cb:define track="4" />
    <cb:define even-odd="Even" />
    <cb:reqBuild req-number="REQ009" />
    <cb:reqBuild req-number="REQ023" />
    <cb:reqBuild req-number="REQ194" />
    <cb:reqBuild req-number="REQ022" queue-priority="1950" />
  </cb:define>

  <cb:define name="track5">
    <cb:define track="5" />
    <cb:define even-odd="Odd" />
    <cb:reqBuild req-number="REQ047" />
  </cb:define>

  <cb:define name="build3Tracks2094">
    <cb:define customer="2094" />
    <cb:track2 />
    <cb:track4 />
  </cb:define>

  <cb:define name="build2Tracks2094">
    <cb:define customer="2094" />
    <cb:track1 />
    <cb:track3 />
    <cb:track5 />
  </cb:define>

<cb:define name="build2TBSAH">
  <cb:define customer="2095" />
  <cb:reqBuild track="1" req-number="REQ002" />
  <cb:reqBuild track="1" req-number="REQ004" />
  <cb:reqBuild track="1" req-number="REQ014" />
</cb:define>

<cb:define name="build3TBSAH">
  <cb:define customer="2095" />
