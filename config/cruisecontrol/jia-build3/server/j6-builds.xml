<?xml version="1.0"?>
<cb:config-template xmlns:cb="urn:ccnet.config.builder">
  
  <!-- Source control projects -->
  
  <!-- Note that in newer versions of ccnet, ccnet will create the feature folders. No need to create by hand. -->
  
  <!-- release branches -->
  
  <cb:v76build
      project-name="$(project-root)"
      branch = '7.6.0'
      driver = 'CUST000'
      clean="true"
      build="true"
      data-test="true"
      redis-db="14"
      platform="AnyCpu"
      />

<!-- Disabled until dev gets going  
  <cb:v76build
      project-name="$(project-root)"
      branch = '760_B2'
      driver = 'CUST000'
      clean="true"
      build="true"	
      unit-test="true"
      data-test="false"
      redis-db="13"
      platform="AnyCpu"
      modules="AUTOSHIP"
      />
  -->

  <cb:v76build
      project-name="$(project-root)"
      branch = '760_QA'
      driver = 'CUST000'
      clean="true"
      build="true"	
      unit-test="true"
      data-test="false"
      redis-db="16"
      platform="AnyCpu"
      modules="AUTOSHIP"
      />
  
  <cb:v76build
      project-name="$(project-root)-DeployTest"
      branch = '760_QA'
      driver = 'CUST000'
      clean="true"
      build="true"	
      unit-test="true"
      web-test="true"
      data-test="false"
      deployment-webserver="DEV-8900WEB1"
      deployment-sitename="DEV_8900_760_CA" 
      redis-db="16"
      platform="AnyCpu"
      modules="AUTOSHIP"
      queueSuffix="-WebTest"
      sourceCheckInterval=""
      db-server="DEV-8900sql1"
      />
  
  <cb:v76deployment
      project-name="$(project-root)-DeployTest"
      branch = '760_QA'
      driver = 'CUST000'
      clean="true"
      build="true"	
      unit-test="true"
      web-test="true"
      data-test="false"
      deployment-webserver="DEV-8900WEB1"
      deployment-sitename="DEV_8900_760_CA" 
      redis-db="16"
      platform="AnyCpu"
      modules="AUTOSHIP"
      queueSuffix="-WebTest"
      sourceCheckInterval=""
      db-server="DEV-8900sql1"
      />
  
</cb:config-template>
